<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shasta.todolist" >

    <user-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <receiver android:name=".TimeNotification" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".bye"
            android:label="@string/title_activity_bye" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shasta.todolist.MainActivity" />
        </activity>
        <activity
            android:name=".film"
            android:label="@string/title_activity_film"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shasta.todolist.MainActivity" />
        </activity>
        <activity
            android:name=".read"
            android:label="@string/title_activity_read"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shasta.todolist.MainActivity" />
        </activity>
        <activity
            android:name=".Password"
            android:label="@string/title_activity_password"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shasta.todolist.MainActivity" />
        </activity>
        <activity
            android:name=".todo"
            android:label="@string/title_activity_todo"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shasta.todolist.MainActivity" />
        </activity>
        <activity
            android:name=".toget"
            android:label="@string/title_activity_toget"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.shasta.todolist.MainActivity" />
        </activity>

        <service
            android:name=".MyNotification"
            android:enabled="true"
            android:exported="true" >
        </service>
    </application>

</manifest>
